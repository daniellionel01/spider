{
  "manifest_version": 3,
  "name": "Spider",
  "version": "1.0.0",
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "./build/dev/javascript/spider_extension/background.dist.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "type": "module",
      "js": ["./build/dev/javascript/spider_extension/content.dist.js"]
    }
  ],
  "permissions": [
    "tabs",
    "proxy",
    "alarms",
    "storage",
    "debugger",
    "activeTab",
    "offscreen",
    "webNavigation",
    "unlimitedStorage",
    "scripting"
  ]
}
